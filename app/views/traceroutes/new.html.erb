<h1>Traceroutes#new</h1>
<p>Find me in app/views/traceroutes/new.html.erb</p>



<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="traceroute new">
  <h2>Create Traceroute</h2>

  <%= form_for(:traceroute, :url => {:action => 'create'}) do |f| %>

    <table summary="Traceroute form fields">
      <tr>
        <th>Name</th>
        <td><%= f.text_field(:name) %></td>
      </tr>
      <tr>
        <th>Description</th>
        <td><%= f.text_field(:description) %></td>
      </tr>
      <tr>
        <th>User_id</th>
        <td><%= f.text_field(:user_id) %></td>
      </tr>
    </table>

    <div class="form-buttons">
      <%= submit_tag("Create Traceroute") %>
    </div>

  <% end %>
</div>